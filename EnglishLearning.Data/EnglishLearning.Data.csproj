<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <AssemblyVersion>1.0.1</AssemblyVersion>
    <FileVersion>1.0.1</FileVersion>
    <PackageId>viwik.$(AssemblyName)</PackageId>
    <Version>1.0.1</Version>
    <Authors>vi-wik</Authors>
    <AssemblyName>viwik.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>viwik.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <Description>英语学习数据文件</Description>
    <PackageReleaseNotes>元音与媒体关系部分调整</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/vi-wik/EnglishLearning</PackageProjectUrl>
    <RepositoryUrl>https://github.com/vi-wik/EnglishLearning</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="english.db3" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="english.db3">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

</Project>
